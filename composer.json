{
    "name": "atmosphere/cli",
    "description": "Atmosphere in the CLI.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Romano Schoonheim",
            "email": "support@atmosphere.sh"
        }
    ],
    "bin": [
        "bin/atmosphere"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "Atmosphere\\Cli\\": "src/"
        }
